<vhd:ValueHelpDialog title="Clientes" supportMultiselect="false" supportRanges="false" stretch="true" cancel="inputHelpClientesCancel" supportRangesOnly="false" ok="inputHelpClientesOk" key="id" descriptionKey="nome">
<vhd:filterBar>
<fb:FilterBar reset="onReset" search="onInputHelpClientesFilterSearch" showRestoreButton="false" showClearButton="false" filterBarExpanded="true" showGoOnFB="true">
<fb:filterItems>
<fb:FilterItem name="InputHelpClientesId" label="ID">
<fb:control>
<Input type="Text" placeholder="ID cliente"/>
</fb:control>
</fb:FilterItem>
<fb:FilterItem name="InputHelpClientesNome" label="Nome">
<fb:control>
<Input type="Text" placeholder="Nome cliente"/>
</fb:control>
</fb:FilterItem>
<fb:FilterItem name="InputHelpClientesCpf" label="CPF">
<fb:control>
<MaskInput mask="999.999.999-99" placeholderSymbol="_" placeholder="Digite o CPF (apenas números)"/>
</fb:control>
</fb:FilterItem>
<fb:FilterItem name="InputHelpClientesCnpj" label="CNPJ">
<fb:control>
<MaskInput mask="99.999.999/9999-99" placeholderSymbol="_" placeholder="Digite o CNPJ (apenas números)"/>
</fb:control>
</fb:FilterItem>
</fb:filterItems>
</fb:FilterBar>
</vhd:filterBar>
</vhd:ValueHelpDialog>
</core:FragmentDefinition>
