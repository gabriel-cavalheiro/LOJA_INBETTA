<View xmlns:l="sap.ui.layout" xmlns:u="sap.ui.unified" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt" controllerName="controller.App">
<u:Shell id="myShell">
<u:headItems>
<u:ShellHeadItem id="headMenuButton" tooltip="Configuration" icon="sap-icon://menu2" press="handlePressConfiguration"/>
<u:ShellHeadItem icon="sap-icon://home" press="onNavHome"/>
</u:headItems>
<u:headEndItems>
<u:ShellHeadItem id="headConnectedButton" icon="sap-icon://connected" tooltip="network"/>
<u:ShellHeadItem id="headLogoffButton" icon="sap-icon://log" tooltip="Logout 2" press="handleLogoffPress"/>
</u:headEndItems>
<u:user>
<u:ShellHeadUserItem image="sap-icon://person-placeholder" username="{Profile>/nome}" press="handleUserItemPressed"/>
</u:user>
<u:paneContent>
<SelectList id="sideNavigation" selectedKey="{App>/selectedNav}" itemPress="onNavigationPress" items="{Profile>/tTiles}">
<core:ListItem key="{Profile>navto}" text="{Profile>title}"/>
</SelectList>
</u:paneContent>
<App id="app"> </App>
</u:Shell>
</View>
